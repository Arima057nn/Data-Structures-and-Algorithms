#include <stdio.h>
#include <st

int main(){
  FILE *ptr1,*ptr2;
  ptr1 = fopen("ex4a.txt","w");
  ptr2 = fopen("ex4b.txt","w");
  fputs("hoc C khong he kho",ptr1);
  char c;
  if(ptr1 == NULL || ptr2 = NULL ){
    printf("cannot find file txt");
    exit(1);
  }
  
  while( (c = fgetc(ptr1)) != EOF){
    fputc(ptr2,c);
  }



  return 0;
}
